  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (web)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/ExampleApp/bin/Debug/net7.0/ExampleApp.dll",
      "args": [],
      "cwd": "${workspaceFolder}/ExampleApp",
      "stopAtEntry": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "LOG_PREFIX": "Example App",
        "CONNECTION_STRING": "TEST"
      }
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}
